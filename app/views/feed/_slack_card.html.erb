<activity-card>
  <avatar>
    <img src="http://placekitten.com/g/64/64" alt="avatar" />
  </avatar>
  <action>
    <h3>1 Message on <%= @team_info.channel_id_to_name[feed.try(:channel_id) || ""] %></h3>
    <ul>
      <li>
        <b><%= @team_info.slack_id_to_member_name[feed.try(:user_id) || ""] %></b>:
        <%= feed.text %>
      </li>
    </ul>
  </action>
  <action-icon>
    <timestamp>
      <%= l(feed.timestamp) %>
    </timestamp>
    *icon goes here*
  </action-icon>
</activity-card>
