<activity-card>
  <avatar>
    <img src="http://placekitten.com/g/64/64" alt="avatar" />
  </avatar>
  <action>
    <h3><b>Event: </b> <%= feed.try(:event) || "" %>1 Message on <%= @team_info.channel_id_to_name[feed.try(:channel_id) || ""] %></h3>
    <ul>
      <li>
        <b>Event: </b>
        <%= feed.try(:event) || "" %>
      </li>
      <li>
        <b>Payload: </b>
        <%= feed.try(:payload) || "" %>
      </li>
    </ul>
  </action>
  <action-icon>
    <timestamp>
      <%= l(feed.timestamp) %>
    </timestamp>
    *icon goes here*
  </action-icon>
</activity-card>
